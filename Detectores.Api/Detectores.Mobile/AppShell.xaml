<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Detectores.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Detectores.Mobile"
    Shell.FlyoutBehavior="Disabled"
    Title="Detectores.Mobile">


    <ShellContent
        ContentTemplate="{DataTemplate local:LoginPage}" Route="LoginPage"></ShellContent>
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
