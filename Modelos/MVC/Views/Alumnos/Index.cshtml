@model IEnumerable<Modelos.Alumno>

@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>

    @Html.ActionLink("Crear", "Create")
    <table class="table">    
    <tr>
        <td>Codigo</td>
        <td>Nombre</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Codigo</td>
            <td>@item.Nombre</td>
        </tr>
    }
    </table>

</div>